                </div>
              </div>
              
              <div class="form-row">
                <div class="form-group col-md-2">
                  戶政事務所
                </div>
                <div class="form-group col" style="background-color:#f0f0f0;">
                  <%= @yfcase.township.try(:land_office) %>戶政事務所
                </div>
              </div>
              
