                    <%= f.text_field :creditorIdentitycard , placeholder: '身分証或統一編號', class: 'form-control' %>
                  </div>
                  <div class="form-group col-md-2">
                    <%= f.date_field :creditorBirthday , placeholder: '生日', class: 'form-control' %>
                  </div>
