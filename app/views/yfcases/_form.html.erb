                    <%= f.text_field :creditorNumber , placeholder: '號', class: 'form-control' %>
                  </div>
                  <div class="form-group col-md-2">
                    <%= f.text_field :creditorFloor , placeholder: '樓', class: 'form-control' %>
                  </div>
