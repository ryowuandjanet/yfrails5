                    <%= f.text_field :creditorLocallphone , placeholder: '市話', class: 'form-control' %>
                  </div>
                  <div class="form-group col-md-2">
                    <%= f.text_field :creditorMobilelphone , placeholder: '手機號碼', class: 'form-control' %>
                  </div>
                  <div class="form-group col-md-4">
                    <%= f.text_field :creditorNotes , placeholder: '備註', class: 'form-control' %>
                  </div>
                </div>
                <div class="form-row">
                  <div class="form-group col-md-2">
                    <%= f.text_field :creditorCountry , placeholder: '縣市', class: 'form-control' %>
                  </div>
                  <div class="form-group col-md-2">
                    <%= f.text_field :creditorTownship , placeholder: '鄉鎮', class: 'form-control' %>
                  </div>
                  <div class="form-group col-md-2">
                    <%= f.text_field :creditorVillage , placeholder: '村里', class: 'form-control' %>
                  </div>
                  <div class="form-group col-md-1">
                    <%= f.text_field :creditorNeighbor , placeholder: '鄰', class: 'form-control' %>
                  </div>
                  <div class="form-group col-md-2">
                    <%= f.text_field :creditorStreet , placeholder: '街路', class: 'form-control' %>
                  </div>
                  <div class="form-group col-md-1">
                    <%= f.text_field :creditorSection , placeholder: '段', class: 'form-control' %>
                  </div>
                  <div class="form-group col-md-1">
                    <%= f.text_field :creditorLane , placeholder: '巷', class: 'form-control' %>
                  </div>
                  <div class="form-group col-md-1">
                    <%= f.text_field :creditorAlley , placeholder: '弄', class: 'form-control' %>
                  </div>
                </div>
                <div class="form-row">
