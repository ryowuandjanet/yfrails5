<%= form_with(model: yfcase, local: true) do |f| %>

  <% if yfcase.errors.any? %>
    <div id="error_explanation">
        <h2>
            <%= pluralize(yfcase.errors.count, "error") %> prohibited this yfcase from being saved:</h2>
        <ul>
            <% yfcase.errors.full_messages.each do |message| %>
            <li>
                <%= message %>
            </li>
            <% end %>
        </ul>
    </div>
  <% end %>
  
  <!--是否要歸類到過往記錄-->
  <div class="col-md-2">
    <td><%= f.check_box :now_or_before %></td>
    <td><%= f.label :now_or_before,"歸類到過往記錄" %></td>
  </div>

  <div class="accordion" id="accordionExample">
    <% if yfcase.priority_purchase_result.present? && yfcase.priority_purchase_result == "無人優購" %>
      <!--無人優購後顯示：不動產登記-->
