                  </div>
                  <div class="form-group col-md-2">
                    <%= f.text_field :debtorBuildholdingpointall , placeholder: '建物所有持分', class: 'form-control' %>
                  </div>
                </div>
              </div>
                
              <hr>
                
              <!--權利人-->
