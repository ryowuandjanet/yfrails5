<style>
  #collapseRealestaterregistration div.form-group.col-md-2 {
    margin:0px;
  }
  .card-header {
    padding:2px;
  }
  .card{
    background-color:Gainsboro;
    border:1px solid black;
  }
  .card-body{
    background-color:white;
  }
  #relatedpersonnels {
    width:100%;
  }
  .nested-fields .field{
    padding:10px;
  }
  .nested-fields .form-row{
    padding:0px;
    margin:0px;
  }

