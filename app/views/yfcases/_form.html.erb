                  <div class="form-group col-md-2">
                    <%= f.text_field :creditorLandholdingpointall, :value => "#{yfcase.debtorLandholdingpointall}" , placeholder: '土地所有持分', class: 'form-control' %>
                  </div>
                  <div class="form-group col-md-2">
                    <%= f.text_field :creditorBuildholdingpointperson, :value => "#{yfcase.debtorBuildholdingpointperson}" , placeholder: '建物個人持分', class: 'form-control' %>
                  </div>
                  <div class="form-group col-md-2">
                    <%= f.text_field :creditorBuildholdingpointall, :value => "#{yfcase.debtorBuildholdingpointall}" , placeholder: '建物所有持分', class: 'form-control' %>
                  </div>
                </div>
              </div>
              
              <hr>  
                
              <!--共有人-->
              <h3>共有人 : </h3>
              <div>
                <%= f.check_box :co_owner %>
