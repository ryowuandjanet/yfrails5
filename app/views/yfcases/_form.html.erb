                    <%= f.text_field :debtorLocallphone , placeholder: '市話', class: 'form-control' %>
                  </div>
                  <div class="form-group col-md-2">
                    <%= f.text_field :debtorMobilelphone , placeholder: '手機號碼', class: 'form-control' %>
                  </div>
                  <div class="form-group col-md-4">
                    <%= f.text_field :debtorNotes , placeholder: '備註', class: 'form-control' %>
                  </div>
                </div>
                <div class="form-row">
                  <div class="form-group col-md-2">
                    <%= f.text_field :debtorCountry , placeholder: '縣市', class: 'form-control' %>
                  </div>
                  <div class="form-group col-md-2">
                    <%= f.text_field :debtorTownship , placeholder: '鄉鎮', class: 'form-control' %>
                  </div>
                  <div class="form-group col-md-2">
                    <%= f.text_field :debtorVillage , placeholder: '村里', class: 'form-control' %>
                  </div>
                  <div class="form-group col-md-1">
                    <%= f.text_field :debtorNeighbor , placeholder: '鄰', class: 'form-control' %>
                  </div>
                  <div class="form-group col-md-2">
                    <%= f.text_field :debtorStreet , placeholder: '街路', class: 'form-control' %>
                  </div>
                  <div class="form-group col-md-1">
                    <%= f.text_field :debtorSection , placeholder: '段', class: 'form-control' %>
                  </div>
                  <div class="form-group col-md-1">
                    <%= f.text_field :debtorLane , placeholder: '巷', class: 'form-control' %>
                  </div>
                  <div class="form-group col-md-1">
                    <%= f.text_field :debtorAlley , placeholder: '弄', class: 'form-control' %>
                  </div>
                </div>
                <div class="form-row">
